{
  "name": "yup-field-props-base",
  "version": "1.0.0-alpha.1",
  "description": "Get the validation properties for a field in a yup schema",
  "files": [
    "dist/"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "types:check": "tsc --project tsconfig.json",
    "format:check": "prettier \"**/*\" --check --ignore-unknown",
    "format:fix": "npm run format:check -- --write",
    "lint": "eslint . --max-warnings 17",
    "check": "npm run types:check && npm run format:check && npm run lint",
    "prebuild": "npm run check && npx rimraf -rf dist",
    "build": "tsc --project tsconfig.production.json && rollup --config",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test:coverage"
  },
  "keywords": [
    "yup",
    "field",
    "props",
    "validation",
    "schema",
    "describe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kfrankot/yup-field-props-base.git"
  },
  "author": "Kevin Frankot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kfrankot/yup-field-props-base/issues"
  },
  "homepage": "https://github.com/kfrankot/yup-field-props-base#readme",
  "devDependencies": {
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.24.7",
    "@babel/plugin-transform-optional-chaining": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.4.8",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-license": "^3.5.2",
    "ts-jest": "^29.1.5",
    "type-fest": "^4.25.0",
    "typescript": "^5.5.3",
    "yup": "^1.4.0"
  },
  "peerDependencies": {
    "yup": "^1.0.0"
  }
}
